{
  "name": "aws-ip-ranges",
  "version": "1.0.0",
  "description": "AWS Lambda function to collect IP ranges",
  "main": "index.js",
  "repository": "git@github.com:dmgress/aws-ip-ranges.git",
  "author": "Daniel Gressmann <dmgressmann@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "aws-xray-sdk": "^3.10.3"
  },
  "devDependencies": {
    "ansi-styles": "^6.2.1",
    "aws-sdk": "^2.1692.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2"
  },
  "scripts": {
    "lint": "eslint *.js test/ && prettier --check --loglevel warn *.js test/",
    "postinstall": "husky install",
    "prepare": "husky install && chmod ug+x .husky/*",
    "test": "jest --coverage --runInBand"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/test/**"
    ]
  }
}
