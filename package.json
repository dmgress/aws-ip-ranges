{
  "name": "aws-ip-ranges",
  "version": "1.0.0",
  "description": "AWS Lambda function to collect IP ranges",
  "main": "index.js",
  "repository": "git@github.com:dmgress/aws-ip-ranges.git",
  "author": "Daniel Gressmann <dmgressmann@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "aws-xray-sdk": "^3.12.0"
  },
  "devDependencies": {
    "ansi-styles": "^6.2.3",
    "aws-sdk": "^2.1693.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4"
  },
  "scripts": {
    "lint": "eslint *.js test/ && prettier --check --loglevel warn *.js test/",
    "postinstall": "husky install",
    "prepare": "husky install && chmod ug+x .husky/*",
    "test": "jest --coverage --runInBand"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/test/**"
    ]
  }
}
